{
  "name": "baks-dev/core",
  "description": "Модуль Core",
  "homepage": "https://github.com/baks-dev/core",
  "keywords": [
    "symfony",
    "bundle",
    "library",
    "plugin",
    "module",
    "core",
    "system",
    "baksdev"
  ],
  "license": "MIT",
  "type": "symfony-bundle",
  "authors": [
    {
      "name": "Baks Development",
      "email": "admin@baks.dev"
    }
  ],
  "require": {
    "ext-mbstring": "*",
    "php": ">=8.4",
    "symfony/lock": "^7.3|^8.0",
    "symfony/form": "^7.3|^8.0",
    "symfony/scheduler": "^7.3|^8.0",
    "symfony/messenger": "^7.3|^8.0",
    "symfony/console": "^7.3|^8.0",
    "symfony/translation": "^7.3|^8.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest"
  },

  "require-dev": {
    "doctrine/doctrine-fixtures-bundle": "^4.3",
    "phpunit/phpunit": "^11.1",
    "symfony/browser-kit": "^7.3|^8.0",
    "symfony/css-selector": "^7.3|^8.0",
    "symfony/debug-bundle": "^7.3|^8.0",
    "symfony/phpunit-bridge": "^7.3|^8.0",
    "symfony/stopwatch": "^7.3|^8.0",
    "symfony/var-dumper": "^7.3|^8.0",
    "symfony/web-profiler-bundle": "^7.3|^8.0"
  },


  "autoload": {
    "psr-4": {
      "BaksDev\\Core\\": ""
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "symfony/flex": true
    }
  }
}